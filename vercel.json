{
    "build": {
      "env": {
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_KEY": "@supabase_key"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1",
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate"
        }
      }
    ]
}